import java.util.*; 
class Solution 
{
public static void main(String[] args)
{
Scanner sc=new Scanner(System.in);
System.out.print("Enter n");
int n=sc.nextInt();
int i,j; int count=0;
System.out.print("Enter elements");
int ar[]=new int[n];
for(i=0;i<n;i++)
{
 ar[i]=sc.nextInt(); 
}
for(i=0;i<n;i++)
{
  if(i!=n-1)
 {
  System.out.print(ar[i]);
  }
  else
  {
  	System.out.print(ar[i]+1);
  	}
}
}
}
