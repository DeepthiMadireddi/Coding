import java.util.*;
class Factorial
{
int n;
int  fact(int n)
{
if(n==1)
{
return 1;
}
else if(n==0)
{
	return 0;
}
else
{
return (n*fact(n-1));
}
}
}
class main
{
public static void main(String args[])
{
Scanner sc=new Scanner (System.in);
System.out.print("enter number");
int count=0;
int n=sc.nextInt();
Factorial f=new Factorial();
//System.out.print(f.fact(n));
if((f.fact(n)%10==0)&&(n!=0))
{
count++;
}
System.out.println(count);
}
}
