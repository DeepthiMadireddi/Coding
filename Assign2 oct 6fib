import java.util.*;
class assgn2
{
    public static int fib(int n)
    {
        if (n <2) 
        {
            return n;
        }
 
        return fib(n - 1) + fib(n - 2);
    }
 
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int q=sc.nextInt();
        for (int i=0;i<q;i++)
        {
        int n=sc.nextInt();
    System.out.println("Nth term= " + fib(n));
    }
}
}
