class  Array3
{ 
public static void main(String arg[]) 
{ 
Scanner sc=new Scanner(System.in);
 int size,i; 
System.out.println("Enter array size:");
 size=sc.nextInt(); //6
int ar[]; ar=new int[size]; 
System.out.println("Enter array elements:"); 
for(i=0;i<size;i++) 
{
ar[i]=sc.nextInt(); // 1 0 2 0 3 0
}
for(i=0;i<size;i++)
{
if(ar[i]>0)//1>0;2>0;3>0
{
System.out.print(ar[i]+" ");//1 2 3
}
}
for(i=0;i<size;i++)
{
if(ar[i]==0)//0=0
{
System.out.print(ar[i]+" ");//0
}
}
}
}
