import java.util.*;
 class stack
 { 
 public static void main(String args[]) 
 { 
 Scanner sc=new Scanner(System.in);
 System.out.print("Enter string");
 String str=sc.next();
 int n=str.length();
  char ch[]=str.toCharArray(); 
Stack<Character>st=new Stack<Character>(); 
for(int i=0;i<n;i++)
   { st.push(ch[i]); } 
  for(int i=0;i<n;i++) 
   { 
   char x=st.pop(); System.out.print(x); 
   } } }
