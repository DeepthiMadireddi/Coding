import java.util.*;
class convert
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
System.out.println("Enter string");
String str=sc.nextLine();
String ch[]=str.split(" ");
int n=ch.length;
for(int i=0;i<n;i++)
{
String s = ch[i].substring(0,1).toUpperCase() +ch[i].substring(1);
System.out.print(s+" ");
}
}
}
